												<xs:attribute name="cref" type="xs:string" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element minOccurs="0" name="exception">
											<xs:complexType>
												<xs:attribute name="cref" type="xs:string" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" type="xs:string" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="name" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
